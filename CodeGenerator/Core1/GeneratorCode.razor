<CascadingValue Value="CodeGen">
	<CascadingValue Value="CodeGenHints">
		@ChildContent
	</CascadingValue>
</CascadingValue>

@code {
	[Parameter] public required CodeGeneratorTemplate CodeGen { get; set; }

    [Parameter]
    public bool CodeGenHints { get; set; } = false;

    [Parameter]
    public required RenderFragment ChildContent { get; set; }

}
